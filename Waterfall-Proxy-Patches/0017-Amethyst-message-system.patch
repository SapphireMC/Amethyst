+# Amethyst messages file
+# Generated on ${project.version}
+
+# Command messages
+command:
+  # Alert command
+  alert:
+    success: "8[&4Alert&8]&r "
+    error:
+      message-needed: "&cYou must supply a message."
+
+  # Find command
+  find:
+    success: "&a{0} &ris online at {1}"
+
+  # Ip command
+  ip:
+    success: "&9IP of {0} is {1}"
+
+  # List command
+  list:
+    header: "Available servers:"
+    entry: "&a[{0}] &e({1}): &r{2}"
+    total: "Total players online: {0}"
+
+  # Perms command
+  perms:
+    groups: "&6You have the following groups: {0}"
+    permissions: "=&9- {0}"
+
+  # Reload command
+  reload:
+    success: "&c&lAmethyst has been reloaded. This is NOT advisable and you will not be supported with any issues that arise! Please restart Amethyst ASAP."
+
+  # Send command
+  send:
+    attempt: "&2Attempting to send {0} players to {1}"
+    result: "Send results:"
+    target: "&6Summoned to {0} by {1}"
+    usage: "&cNot enough arguments, usage: /send <server|player|all|current> <target>"
+
+  # Server command
+  server:
+    current: "&6You are currently connected to {0}."
+    available: "&6You may connect to the following servers at this time: "
+    hover: |-
+      {0} players online
+      Click to connect to this server
+
+# Event messages
+event:
+  # Fallback
+  fallback:
+    kick: "&cCould not connect to a default or fallback server, please try again later: {0}"
+    lobby: "&cCould not connect to target server, you have been moved to a fallback server."
+
+  # Restart
+  restart: "[Proxy] Proxy restarting."
+  # Server went down
+  server-down: "&cThe server you were previously on went down, you have been connected to a fallback server"
+
+# Error messages
+error:
+  # Already connect errors
+  already:
+    connected: "&cYou are already connected to this server!"
+    connected-proxy: "&cYou are already connected to this proxy!"
+    connecting: "&cAlready connecting to this server!"
+
+  # Command errors
+  command:
+    internal: "&cAn internal error occurred whilst executing this command, please check the console log for details."
+    not-found: "&cCommand not found!"
+
+  # Json errors
+  json:
+    player: "&cAn error occurred while parsing your message. (Hover for details)"
+    console: "&cAn error occurred while parsing your message: {0}"
+
+  # Permission errors
+  no-perms:
+    command: "&cYou do not have permission to execute this command!"
+    server: "&cYou don't have permission to access this server."
+
+  # Outdated errors
+  outdated:
+    client: "Outdated client! Please use {0}"
+    server: "Outdated server! I'm still on {0}"
+
+  connect-kick: "&cKicked whilst connecting to {0}: {1}"
+  illegal-chat-characters: "&cIllegal characters in chat ({0})"
+  lost-connection: "[Proxy] Lost connection to server."
+  mojang-fail: "Error occurred while contacting login servers, are they down?"
+  name-invalid: "Username contains invalid characters."
+  no-server: "&cThe specified server does not exist."
+  not-authenticated: "Not authenticated with Minecraft.net"
+  not-online: "&cThat user is not online."
+  ping-cannot-connect: "&c[Bungee] Can't connect to server."
+  players-only: "&cOnly in game players can use this command"
+  proxy-full: "Server is full!"
+  timeout: "Server not reachable (timeout). Offline? Incorrectly configured address/port/firewall?"
+  username-needed: "&cPlease follow this command by a user name."
